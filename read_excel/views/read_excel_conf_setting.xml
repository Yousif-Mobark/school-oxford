<odoo>
  <data>
    <!-- Add General Manager Approval  -->
    <record id="res_company_custom_form_view" model="ir.ui.view">
      <field name="name">res.company.custom.form.view</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form"/>
      <field name="arch" type="xml">
          <xpath expr="//form/sheet/notebook" position='inside'>
            <page string="Attendence Configration">            
                <group colspan="2" col="4">
                    <field name="device_id_col_name"     />
                    <field name="checkIn_date_col_name"  />
                    <field name="checkIn_time_col_name"  />
                    <field name="checkOut_date_col_name" />
                    <field name="checkOut_time_col_name" />
                    <field name="time_zone"/>
                </group>
            </page>
          </xpath>
      </field>
    </record>

  </data>
</odoo>